<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oa.dao.CountMapper">
    <resultMap id="BaseResultMap" type="com.oa.model.Count">
        <result column="assessment_id" jdbcType="INTEGER" property="assessmentId"/>
        <result column="user_id" jdbcType="INTEGER" property="userId"/>
        <result column="column_id" jdbcType="INTEGER" property="columnId"/>
        <result column="count" jdbcType="INTEGER" property="count"/>
    </resultMap>
    <insert id="insert" parameterType="com.oa.model.Count">
    insert into count (assessment_id, user_id, column_id, 
      count)
    values (#{assessmentId,jdbcType=INTEGER}, #{userId,jdbcType=INTEGER}, #{columnId,jdbcType=INTEGER}, 
      #{count,jdbcType=INTEGER})
  </insert>
    <insert id="insertSelective" parameterType="com.oa.model.Count">
        insert into count
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="assessmentId != null">
                assessment_id,
            </if>
            <if test="userId != null">
                user_id,
            </if>
            <if test="columnId != null">
                column_id,
            </if>
            <if test="count != null">
                count,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="assessmentId != null">
                #{assessmentId,jdbcType=INTEGER},
            </if>
            <if test="userId != null">
                #{userId,jdbcType=INTEGER},
            </if>
            <if test="columnId != null">
                #{columnId,jdbcType=INTEGER},
            </if>
            <if test="count != null">
                #{count,jdbcType=INTEGER},
            </if>
        </trim>
    </insert>
</mapper>